import java.util.Scanner;
class Patternsqare {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("enter n:");
        int n = input.nextInt();
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                System.out.print(" $ ");
            }
            System.out.println();
        }
    }
}
