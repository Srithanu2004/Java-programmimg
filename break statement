import java.util.Scanner;
class Numbers {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("enter:");
        int n = input.nextInt();
        for (int i = 1; i <= 10; i++) {
            System.out.println(i);
            if (i == 6) {
                break;
            }
            System.out.println(i);
            System.out.println("loop terminated");
        }
    }
